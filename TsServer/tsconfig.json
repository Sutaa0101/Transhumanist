{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "sourceMap": false,
        "declaration": true,
        "outDir": "../Server",
        "rootDir": "../TsServer",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noEmitOnError": true
    }
}
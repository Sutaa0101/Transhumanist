{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "sourceMap": false,
        "declaration": true,
        "outDir": "./Js",
        "rootDir": "./Ts",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noEmitOnError": true
    }
}